<!-- AuroraWeb.UserAccountService -->
<Project frameworkVersion="v3_5" name="AuroraWeb.OpenSimServices" path="addon-modules/AuroraWeb.OpenSimServices" type="Library">
  <Configuration name="Debug">
    <Options>
      <OutputPath>../../bin/</OutputPath>
    </Options>
  </Configuration>
  <Configuration name="Release">
    <Options>
      <OutputPath>../../bin/</OutputPath>
    </Options>
  </Configuration>

  <ReferencePath>../../bin/</ReferencePath>
  <Reference name="System"/>
  <Reference name="System.Core"/>
  <Reference name="System.Web"/>
  <Reference name="System.Xml"/>

  <Reference name="Nini" path="../../bin/"/>
  
  <Reference name="log4net" path="../../bin/"/>
  <Reference name="OpenMetaverse" path="../../bin/"/>
  <Reference name="OpenMetaverseTypes" path="../../bin/"/>
  <Reference name="OpenMetaverse.StructuredData" path="../../bin/"/>
  <Reference name="OpenSim.Services"/>
  <Reference name="Aurora.Framework"/>
  <Reference name="AuroraWeb.Data"/>
  <Reference name="AuroraWeb.Data.MySQL"/>
  <Reference name="AuroraWeb.Loginservice"/>
  <Reference name="AuroraWeb.Utils"/>

  <Files>
    <Match pattern="*.cs" recurse="true">
      <Exclude pattern="Tests" />
    </Match>
  </Files>
</Project>
